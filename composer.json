{
  "name": "famcare/nova-table-field",
  "description": "Table field for Laravel Nova",
  "keywords": [
    "laravel",
    "nova",
    "tables",
    "optimistdigital"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Kaspar Rosin",
      "email": "kaspar.rosin@gmail.com",
      "role": "Developer"
    },
    {
      "name": "Optimist Digital",
      "email": "info@optimistdigital.com",
      "role": "Maintainer"
    },
    {
      "name": "Tarvo Reinpalu",
      "email": "tarvoreinpalu@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.1.0",
    "laravel/nova": "^1.0 || ^2.0 || ^3.0|| ^4.0",
    "famcare/nova-tailwind": "dev-main"
  },
  "autoload": {
    "psr-4": {
      "OptimistDigital\\NovaTableField\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "OptimistDigital\\NovaTableField\\FieldServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:famcare/nova-tailwind.git"
    }
  ]
}
